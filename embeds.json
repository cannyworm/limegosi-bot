    "templates" : {
        "std" : {
            "author": {
                "name": "$author.name$",
                "icon_url": "https://i.imgur.com/HQWULmW.jpeg"
            },
            "title": "$title$",
            "description": "$description$",
            "image": {
                "url" : "$image$"
            },
            "footer": {
                "text": "last check @",
                "icon_url": "https://i.imgur.com/McggFJP.jpeg"
            }
        },
        "full_subject" : {
            "author": {
                "name": "$author.name$",
                "icon_url": "https://i.imgur.com/HQWULmW.jpeg"
            },
            "title": "$subject.name$ [$subject.period$]",
            "description": "$subject.code$ $subject.teacher_name$",
            "fields": [
                {
                    "name": "Start",
                    "value": "$subject.start$",
                    "inline": true
                },
                {
                    "name": "End",
                    "value": "$subject.end$",
                    "inline": true
                },
                {
                    "name": "Google Meet / Zoom",
                    "value": "$subject.meet_link$"
                },
                {
                    "name": "Google Classroom",
                    "value": "$subject.google_classroom$",
                    "inline": true
                },
                {
                    "name": "Extra",
                    "value": "$subject.extra$"
                }
            ],
            "image":  {
                "url" : "$subject.image$"
            },
            "footer": {
                "text": "last check @",
                "icon_url": "https://i.imgur.com/McggFJP.jpeg"
            }
        },
        "min_subject" : {
